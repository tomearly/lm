{
  "name": "angular2-nobullshit-boilerplate",
  "version": "0.0.1",
  "description": "Angular2, TypeScript and Webpack no bullshit!",
  "main": "lib/index.ts",
  "scripts": {
    "devserver": "webpack-dev-server --json --progress",
    "dumpdev": "rm -rf www/* && webpack --progress --colors -d",
    "dumpprod": "rm -rf www/* && webpack --progress --colors --config webpack.config.prod.js -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shprink/angular2-nobullshit-boilerplate.git"
  },
  "author": "shprink <contact@julienrenaux.fr> (http://julienrenaux.fr/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shprink/angular2-nobullshit-boilerplate/issues"
  },
  "homepage": "https://github.com/shprink/angular2-nobullshit-boilerplate#readme",
  "dependencies": {
    "angular2": "2.0.0-beta.16",
    "angular2-google-maps": "^0.5.0",
    "bootstrap": "github:twbs/bootstrap#v4.0.0-alpha",
    "es6-shim": "^0.33.10",
    "node-sass": "^3.4.1",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "3.2.0",
    "css-loader": "0.21.0",
    "file-loader": "0.8.5",
    "html-loader": "0.3.0",
    "html-webpack-plugin": "1.7.0",
    "json-loader": "0.5.7",
    "path": "0.4.10",
    "raw-loader": "3.1.0",
    "sass-loader": "3.2.3",
    "style-loader": "0.13.2",
    "ts-loader": "0.6.1",
    "typescript": "1.7.3",
    "util": "0.10.4",
    "webpack": "1.12.15",
    "webpack-dev-server": "1.10.1"
  }
}
